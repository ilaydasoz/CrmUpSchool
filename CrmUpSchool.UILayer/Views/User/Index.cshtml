@model List<AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_CRMLayout.cshtml";
}


<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Name and Surname</th>
        <th>Gender</th>
        <th>Delete</th>
        <th>Edit</th>
        <th>Details</th>
    </tr>
    @foreach (var item in Model)
            {
<tr>
    <td>@item.Id</td>
    <td>@item.Name @item.Surname</td>
    <td>@item.Email</td>
    <td>@item.Gender</td>
    <td><a href="#" class="btn btn-outline-danger">Delete</a></td>
    <td><a href="#" class="btn btn-outline-success">Update</a></td>
    <td><a href="#" class="btn btn-outline-dark">Details</a></td>
</tr>
            }
</table>
<a href="/Register/Index/" class="btn btn-outline-info">New User Registration</a>